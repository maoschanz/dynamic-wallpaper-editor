[Desktop Entry]
Name=Dynamic Wallpaper Editor
Exec=dynamic-wallpaper-editor %U
Comment=Create or edit dynamic wallpapers for GNOME.
Terminal=false
Type=Application
Categories=GTK;Utility;GNOME;
Keywords=Wallpaper;Daylight;XML;
StartupNotify=true
Icon=com.github.maoschanz.DynamicWallpaperEditor
MimeType=text/xml;application/xml;
Actions=new-window;

[Desktop Action new-window]
Name=New Window
Exec=dynamic-wallpaper-editor --new-window

